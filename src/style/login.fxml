<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" minHeight="206.0" minWidth="350.0" prefHeight="228.0" prefWidth="350.0" spacing="30" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginController">

    <TextField fx:id="username" maxHeight="30" maxWidth="250" minHeight="30" minWidth="100" promptText="Username" />
    <TextField fx:id="pswd" maxHeight="30" maxWidth="250" minHeight="30" minWidth="100" promptText="Password" />

    <HBox alignment="CENTER" spacing="150">
        <Button fx:id="login" onAction="#handleAction" text="Login" wrapText="true" />
        <Button fx:id="register" onAction="#handleAction" text="Register" wrapText="true" />
    </HBox>

</VBox>
